<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
    jcr:primaryType="nt:unstructured">
    <select
        jcr:primaryType="nt:unstructured"
        configurable="{Boolean}false"
        default="{Boolean}true"
        select="{Boolean}true"/>
    <main
        jcr:primaryType="nt:unstructured"
        jcr:title="Title"
        configurable="{Boolean}false"
        default="{Boolean}true"
        sortable="{Boolean}true"/>
    <name
        jcr:primaryType="nt:unstructured"
        jcr:title="Name"
        columnGroup="metadata"
        default="{Boolean}true"
        sortable="{Boolean}true"/>
    <modified
        jcr:primaryType="nt:unstructured"
        jcr:title="Modified"
        columnGroup="metadata"
        default="{Boolean}true"
        sortable="{Boolean}true"
        sortType="date"/>
    <published
        jcr:primaryType="nt:unstructured"
        jcr:title="Published"
        columnGroup="metadata"
        default="{Boolean}true"
        sortable="{Boolean}true"
        sortType="date"/>
    <preview
        jcr:primaryType="nt:unstructured"
        jcr:title="Preview"
        columnGroup="metadata"
        default="{Boolean}true"
        sortable="{Boolean}true"
        sortType="date"/>
    <template
        jcr:primaryType="nt:unstructured"
        jcr:title="Template"
        columnGroup="metadata"
        default="{Boolean}true"
        sortable="{Boolean}true"/>
    <operation
        jcr:primaryType="nt:unstructured"
        jcr:title="Operation"
        columnGroup="metadata"
        default="{Boolean}true"
        sortable="{Boolean}true"/>
    <workflow
        jcr:primaryType="nt:unstructured"
        jcr:title="Workflow"
        columnGroup="metadata"
        default="{Boolean}true"
        sortable="{Boolean}true"/>
    <translationstatus
        jcr:primaryType="nt:unstructured"
        jcr:title="Translated"
        columnGroup="translation"
        default="{Boolean}false"
        sortable="{Boolean}true"/>
    <scPageViews
        jcr:primaryType="nt:unstructured"
        jcr:title="Page Views"
        columnGroup="analytics"
        columnResourceType="cq/gui/components/siteadmin/admin/listview/coral/analytics/analyticscell"
        page-info-provider="analytics"
        page-info-provider-property="pageviews"
        sortable="{Boolean}true"
        sortType="number"/>
    <scUniqueVisitors
        jcr:primaryType="nt:unstructured"
        jcr:title="Unique Visitors"
        columnGroup="analytics"
        columnResourceType="cq/gui/components/siteadmin/admin/listview/coral/analytics/analyticscell"
        page-info-provider="analytics"
        page-info-provider-property="visitors"
        sortable="{Boolean}true"
        sortType="number"/>
    <scAverageTimePerPage
        jcr:primaryType="nt:unstructured"
        jcr:title="Time on Page"
        columnGroup="analytics"
        columnResourceType="cq/gui/components/siteadmin/admin/listview/coral/analytics/analyticscell"
        page-info-provider="analytics"
        page-info-provider-property="averagetimespentonpage"
        sortable="{Boolean}true"
        sortType="number"/>
    <order
        jcr:primaryType="nt:unstructured"
        configurable="{Boolean}false"
        fixedWidth="{Boolean}true"
        order="{Boolean}true"/>
    <nodeCount
        jcr:primaryType="nt:unstructured"
        jcr:title="Node Count"
        columnGroup="metadata"
        default="{Boolean}false"
        page-info-provider="nodecount"
        page-info-provider-property="nodeCount"
        sortable="{Boolean}true"
        sortType="number"/>
</jcr:root>
